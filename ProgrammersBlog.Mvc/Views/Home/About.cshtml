@model ProgrammersBlog.Entities.Concrete.AboutUsPageInfo
@{
    Layout="_AboutLayout";
    ViewBag.Title = "Hakkında";
    ViewBag.Description = Model.SeoDescription;
    ViewBag.Tags = Model.SeoTags;
    ViewBag.Author = Model.SeoAuthor;
}
<div class="conteiner-fluid">
    <div class="jumbotron mt-4">
        <div class="d-flex alian-items-center h-100">
            <div class="conteiner text-center py-5">
                <img src="~/img/userImages/alikagan_870_58_57_15_8_8_2022.png"  alt="ProgrammersBlog Logo" class="img-fluid mb-1"/>
                <div class="conteiner mt-4 mb-5">
                    <h2 class="text-center py-3 mb-5">
                        @Model.Header
                    </h2>
                    <div class="text-center">
                        <p>@Model.Content</p>
                    </div>

                </div>

            </div>

        </div>

    </div>

</div>
